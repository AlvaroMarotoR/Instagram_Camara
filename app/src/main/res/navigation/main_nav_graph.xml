<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.insta.views.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_fragmentCamera3"
            app:destination="@id/fragmentCamera3" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.insta.views.fragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.insta.views.fragments.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" />
    <fragment
        android:id="@+id/recentFragment"
        android:name="com.example.insta.views.fragments.RecentFragment"
        android:label="fragment_recent"
        tools:layout="@layout/fragment_recent" />
    <fragment
        android:id="@+id/addFragment"
        android:name="com.example.insta.views.fragments.AddFragment"
        android:label="fragment_add"
        tools:layout="@layout/fragment_add" />
    <fragment
        android:id="@+id/fragmentCamera3"
        android:name="com.example.insta.activities.FragmentCamera"
        android:label="FragmentCamera" >
        <action
            android:id="@+id/action_fragmentCamera3_to_fragmentCamera22"
            app:destination="@id/fragmentCamera22" />
    </fragment>
    <fragment
        android:id="@+id/fragmentCamera22"
        android:name="com.example.insta.activities.FragmentCamera2"
        android:label="fragment_camera2"
        tools:layout="@layout/fragment_camera2" />
</navigation>